{
  "name": "styled-react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal & Modern boilerplate for building apps with React & styled-components",
  "scripts": {
    "start": "webpack-dev-server --hot --open --mode development",
    "build": "webpack --mode production",
    "now-build": "npm run build",
    "test": "xo && stylelint './src/**/*.js'"
  },
  "main": "src/index.js",
  "repository": "https://github.com/xxczaki/styled-react-boilerplate",
  "author": "Antoni Kepinski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "img-loader": "^3.0.1",
    "offline-plugin": "^5.0.6",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "modern-normalize": "^0.5.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.1",
    "styled-components": "^4.1.3",
    "typeface-open-sans": "0.0.54"
  },
  "xo": {
    "nodeVersion": ">=10",
    "parser": "babel-eslint",
    "envs": [
      "node",
      "browser"
    ],
    "extends": "xo-react",
    "settings": {
      "react": {
        "version": "16.8"
      }
    },
    "rules": {
      "import/no-unassigned-import": "off"
    }
  }
}
